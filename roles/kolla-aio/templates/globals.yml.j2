---
kolla_base_distro: "centos"
kolla_install_type: "binary"
openstack_version: "{{ kolla_version }}"
kolla_internal_vip_address: "{{ ansible_default_ipv4.address }}"
network_interface: "{{ workstation_primary_netif }}"
neutron_external_interface: "{{ kolla_external_interface }}"
neutron_plugin_agent: "openvswitch"
nova_console: "novnc"

enable_haproxy: "no"
enable_aodh: "no"
enable_barbican: "no"
enable_ceilometer: "no"
enable_central_logging: "no"
enable_ceph: "no"
enable_ceph_rgw: "no"
enable_cinder: "yes"
enable_cinder_backend_iscsi: "no"
enable_cinder_backend_lvm: "no"
enable_cloudkitty: "no"
enable_congress: "no"
enable_etcd: "no"
enable_gnocchi: "no"
enable_grafana: "no"
enable_heat: "yes"
enable_horizon: "yes"
enable_influxdb: "no"
enable_ironic: "no"
enable_kuryr: "no"
enable_magnum: "yes"
enable_manila: "no"
enable_mistral: "no"
enable_mongodb: "no"
enable_murano: "no"
enable_multipathd: "no"
enable_neutron_dvr: "no"
enable_neutron_lbaas: "no"
enable_neutron_qos: "no"
enable_neutron_agent_ha: "no"
enable_neutron_vpnaas: "no"
enable_rally: "no"
enable_sahara: "no"
enable_senlin: "no"
enable_swift: "no"
enable_telegraf: "no"
enable_tempest: "no"
enable_watcher: "no"

glance_backend_ceph: true
cinder_backend_ceph: true
nova_backend_ceph: true

